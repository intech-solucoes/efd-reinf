<?xml version="1.0" encoding="UTF-8"?>
<Reinf xmlns="http://www.reinf.esocial.gov.br/schemas/envioLoteEventos/v1_04_00" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <loteEventos>
    {{for evento in eventos}} <evento id="{{evento.id}}">
      <Reinf xmlns="http://www.reinf.esocial.gov.br/schemas/evtTabProcesso/v1_04_00">
        <evtTabProcesso id="{{evento.id}}">
          <ideEvento>
            <tpAmb>{{evento.ind_ambiente_envio}}</tpAmb>
            <procEmi>1</procEmi>
            <verProc>{{evento.versao}}</verProc>
          </ideEvento>
          <ideContri>
            <tpInsc>{{evento.ind_tipo_inscricao}}</tpInsc>
            <nrInsc>{{evento.cod_cnpj_cpf}}</nrInsc>
          </ideContri>
          <infoProcesso>
            {{evento.abertura_tag_operacao}}
            <ideProcesso>
                <tpProc>{{evento.ind_tipo_processo}}</tpProc>
                <nrProc>{{evento.num_processo}}</nrProc>
                <iniValid>{{evento.dta_inicio_validade}}</iniValid>
                <fimValid>{{evento.dta_fim_validade}}</fimValid>
                <indAutoria>{{evento.ind_autoria_judicial}}</indAutoria>
                <infoSusp>
                  <codSusp>{{evento.cod_suspensao}}</codSusp>
                  <indSusp>{{evento.ind_suspensao}}</indSusp>
                  <dtDecisao>{{evento.dta_decisao}}</dtDecisao>
                  <indDeposito>{{evento.ind_deposito_judicial}}</indDeposito>
                </infoSusp>
                <dadosProcJud>
                  <ufVara>{{evento.cod_uf_vara}}</ufVara>
                  <codMunic>{{evento.cod_municipio_vara}}</codMunic>
                  <idVara>{{evento.cod_identificacao_vara}}</idVara>
                </dadosProcJud>
              </ideProcesso>
            {{evento.fechamento_tag_operacao}}
          </infoProcesso>
        </evtTabProcesso>
      </Reinf>
    </evento>{{end}}
  </loteEventos>
</Reinf>